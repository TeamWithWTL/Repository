@charset "utf-8";
/* CSS Document */
/*================管理管理中心布局.开始===================*/
.header {
    position: fixed;
    top: 0;
    right: 0;
    width: 100%;
    height: 42px;
    z-index: 11;

}

.header-box {
    margin: 0 auto;
    min-width: 780px;
}

.main-sidebar {
    position: absolute;
    top: 135px;
    bottom: 0;
    left: 0;
    width: 181px;
}

    .main-sidebar iframe {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }

.sidebar-nav {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    width: 100%;
}

.main-container {
    position: absolute;
    top: 135px;
    right: 0;
    bottom: 0;
    /*left: 181px;*/
    left:0;
}

    .main-container iframe {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }
/*================管理管理中心布局.结束===================*/

